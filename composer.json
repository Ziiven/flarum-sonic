{
    "name": "ganuonglachanh/sonic",
    "description": "Allows to search by sonic",
    "keywords": ["search","flarum","sonic"],
    "type": "flarum-extension",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "GaNuongLaChanh",
            "email": "ganuonglachanh@outlook.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/ganuonglachanh/flarum-sonic/issues",
        "source": "https://github.com/ganuonglachanh/flarum-sonic"
    },
    "require": {
        "flarum/core": ">=1.0",
        "ppshobi/psonic": "^2.0.1"
    },
    "extra": {
        "flarum-extension": {
            "title": "Search By Sonic",
            "icon": {
                "name": "fa fa-search",
                "backgroundColor": "#1976D2",
                "color": "#f44336"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "GaNuongLaChanh\\Sonic\\": "src/"
        }
    }
}
